<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tokudai0000.tokumemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TokumemoAndroid">

        <activity android:name="com.tokudai0000.tokumemo.View.Main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.tokudai0000.tokumemo.View.Menu.MenuActivity"
            android:theme="@style/Theme.AppCompat.Translucent">
        </activity>

        <activity android:name="com.tokudai0000.tokumemo.View.Agreement.AgreementActivity">
        </activity>

        <activity android:name="com.tokudai0000.tokumemo.View.Favorite.FavoriteActivity">
        </activity>

        <activity android:name="com.tokudai0000.tokumemo.View.Menu.AboutThisApp.AboutThisAppActivity">
        </activity>

        <activity android:name="com.tokudai0000.tokumemo.View.Menu.Customize.CustomizeActivity">
        </activity>

        <activity android:name="com.tokudai0000.tokumemo.View.Menu.FirstView.FirstViewActivity">
        </activity>

        <activity android:name="com.tokudai0000.tokumemo.View.Menu.Password.PasswordActivity">
        </activity>

        <activity android:name="com.tokudai0000.tokumemo.View.Menu.Syllabus.SyllabusActivity">
        </activity>

        <activity android:name="com.tokudai0000.tokumemo.View.PrivacyPolicy.PrivacyPolicyActivity">
        </activity>

        <activity android:name="com.tokudai0000.tokumemo.View.TermsOfService.TermsOfServiceActivity">
        </activity>


    </application>
    // webViewの権限を追加
    <uses-permission android:name="android.permission.INTERNET" />
</manifest>